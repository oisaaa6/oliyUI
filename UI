local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

WindUI:SetTheme("Dark")
WindUI.TransparencyValue = 0.2
local MainWindow = WindUI:CreateWindow({
    Title = "Oliy hub V2",
    Icon = "rocket",
    Size = UDim2.fromOffset(850, 650),
    Folder = "OliyHub_Settings",
    SideBarWidth = 260,
    ScrollBarEnabled = true,
    Background = WindUI:Gradient({
        ["0"] = { Color = Color3.fromHex("#1a1a2e"), Transparency = 0.9 },
        ["100"] = { Color = Color3.fromHex("#16213e"), Transparency = 0.8 }
    }, { Rotation = 45 })
})

MainWindow:Tag({ Title = "V2", Color = Color3.fromHex("#30ff6a") })
MainWindow:Tag({ Title = "Universal", Color = Color3.fromHex("#6366f1") })
MainWindow:CreateTopbarButton("theme-switch", "moon", function()
    local newTheme = WindUI:GetCurrentTheme() == "Dark" and "Light" or "Dark"
    WindUI:SetTheme(newTheme)
    WindUI:Notify({
        Title = "Theme Updated",
        Content = "Switched to " .. newTheme .. " Theme",
        Duration = 2,
        Icon = newTheme == "Dark" and "moon" or "sun"
    })
end, 990)

local function getLocalCharacter()
    local plr = game.Players.LocalPlayer
    if not plr or not plr.Character then
        WindUI:Notify({ Title = "Error", Content = "角色未加载！", Icon = "x", Duration = 3 })
        return nil
    end
    return plr.Character
end

local function loadRemoteScript(url, scriptName)
    local success, err = pcall(function()
        loadstring(game:HttpGet(url, true))()
    end)
    if success then
        WindUI:Notify({
            Title = "Loaded",
            Content = scriptName .. " 加载成功",
            Icon = "check",
            Duration = 3
        })
    else
        WindUI:Notify({
            Title = "Load Failed",
            Content = scriptName .. " 错误: " .. err,
            Icon = "x",
            Duration = 5
        })
    end
end

